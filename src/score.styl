[ai-score]
  text-align center
  >div
    display inline-block
    margin 0 5px
    width 50px
    height 50px
    background url(../assets/images/star-orange.svg)
    background-size contain
for v in 0..10
  [ai-score~={"'"+v+"'"}]
    if v is 0
      >div
        background url(../assets/images/star-gray.svg)
    else
      >div:nth-child({v})~div
        background url(../assets/images/star-gray.svg)